{
  "name": "auwla-compiler",
  "version": "0.2.5",
  "description": "Auwla Framework Compiler - Transforms .auwla files to reactive components",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "auwla-compiler": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite-plugin": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "auwla",
    "compiler",
    "reactive",
    "ui",
    "framework",
    "typescript",
    "vite",
    "plugin"
  ],
  "author": "snrraptopack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/snrraptopack/auwla.git",
    "directory": "packages/auwla-compiler"
  },
  "homepage": "https://github.com/snrraptopack/auwla#readme",
  "bugs": {
    "url": "https://github.com/snrraptopack/auwla/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun run test-route-generator.ts"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}