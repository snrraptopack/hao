{
  "name": "auwla-compiler",
  "version": "0.0.4",
  "description": "Auwla Framework Compiler - Transforms .auwla files to reactive components",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "auwla",
    "compiler",
    "reactive",
    "ui",
    "framework",
    "typescript",
    "vite",
    "plugin"
  ],
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun run test-route-generator.ts"
  },
  "peerDependencies": {
    "auwla": "^0.0.4",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__parser": "^7.1.1",
    "@types/babel__traverse": "^7.20.6",
    "@types/babel__types": "^7.20.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}