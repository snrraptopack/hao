{
  "name": "auwla-vscode",
  "displayName": "Auwla Language Support",
  "description": "Syntax highlighting and basic language features for .auwla files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:auwla",
    "onFileSystem:*/**/*.auwla"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^4.9.0",
    "vsce": "^2.0.1"
  },
  "dependencies": {}
}