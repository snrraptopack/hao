{
  "name": "Auwla",
  "scopeName": "source.auwla",
  "fileTypes": [
    "auwla"
  ],
  "patterns": [
    {
      "name": "meta.page-directive.auwla",
      "match": "^\\s*(//\\s*)(@page)\\s+(.*)$",
      "captures": {
        "1": {
          "name": "comment.line.double-slash.auwla"
        },
        "2": {
          "name": "keyword.control.directive.page.auwla"
        },
        "3": {
          "name": "string.unquoted.route.auwla"
        }
      }
    },
    {
      "name": "comment.line.double-slash.auwla",
      "match": "^\\s*//.*$"
    },
    {
      "begin": "(<)(script)(>)",
      "end": "(</)( script)(>)",
      "beginCaptures": {
        "1": {
          "name": "punctuation.definition.tag.begin.html"
        },
        "2": {
          "name": "entity.name.tag.script.html"
        },
        "3": {
          "name": "punctuation.definition.tag.end.html"
        }
      },
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.tag.begin.html"
        },
        "2": {
          "name": "entity.name.tag.script.html"
        },
        "3": {
          "name": "punctuation.definition.tag.end.html"
        }
      },
      "contentName": "meta.embedded.block.typescript source.ts",
      "patterns": [
        {
          "include": "source.ts"
        }
      ]
    },
    {
      "begin": "(?<=</script>)",
      "end": "\\z",
      "contentName": "meta.embedded.block.jsx source.tsx",
      "patterns": [
        {
          "include": "source.tsx"
        }
      ]
    }
  ]
}