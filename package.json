{
  "name": "auwla",
  "version": "0.1.2",
  "description": "A lightweight, reactive UI framework with blazing-fast performance",
  "type": "module",
  "main": "./dist/auwla.umd.cjs",
  "module": "./dist/auwla.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/auwla.js",
      "require": "./dist/auwla.umd.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    }
  },
  "typesVersions": {
    "*": {
      "jsx-runtime": ["dist/jsx-runtime.d.ts"],
      "jsx-dev-runtime": ["dist/jsx-dev-runtime.d.ts"],
      "integrations/react": ["dist/integrations/react.d.ts"],
      "*": ["dist/index.d.ts"]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "reactive",
    "ui",
    "framework",
    "typescript",
    "lightweight",
    "spa",
    "router"
  ],
  "author": "snrraptopack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/snrraptopack/auwla.git"
  },
  "homepage": "https://github.com/snrraptopack/auwla#readme",
  "bugs": {
    "url": "https://github.com/snrraptopack/auwla/issues"
  },
  "scripts": {
    "dev": "vite",
    "postbuild": "node -e \"require('fs').rmSync('dist/app', { recursive: true, force: true })\"",
    "build:lib": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:app": "vite build --config vite.config.app.ts",
    "preview": "vite preview",
    "preview:app": "vite preview --outDir dist-app",
    "template:parse": "bun run template/test/test-parser.ts",
    "template:test": "bun run template/test/test-full.ts",
    "prepublishOnly": "npm run build",
    "dev:bench:auwla": "npm --prefix apps/auwla-bench run dev",
    "dev:bench:react": "npm --prefix apps/react-bench run dev",
    "dev:bench:vue": "npm --prefix apps/vue-bench run dev",
    "preview:bench:auwla": "npm --prefix apps/auwla-bench run preview",
    "preview:bench:react": "npm --prefix apps/react-bench run preview",
    "preview:bench:vue": "npm --prefix apps/vue-bench run preview"
  },
  "bin": {
    "auwla": "./dist/cli.js",
    "auwla-routes": "./dist/cli.js"
  },
  "devDependencies": {
    "@types/acorn": "^6.0.4",
    "@types/bun": "latest",
    "@types/handlebars": "^4.1.0",
    "handlebars": "^4.7.8",
    "typescript": "latest",
    "vite": "latest",
    "@tailwindcss/vite": "^4.1.14",
    "tailwindcss": "^4.1.14"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {}
}
