{
  "name": "auwla",
  "version": "0.2.5",
  "description": "A lightweight, reactive UI framework with blazing-fast performance",
  "type": "module",
  "main": "./dist/auwla.js",
  "module": "./dist/auwla.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/auwla.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js"
    },
    "./transition": {
      "types": "./dist/transition/index.d.ts",
      "import": "./dist/transition/index.js"
    },
    "./transition/view-transitions.css": {
      "default": "./dist/transition/view-transitions.css"
    }

  },
  "typesVersions": {
    "*": {
      "jsx-runtime": [
        "dist/jsx-runtime.d.ts"
      ],
      "jsx-dev-runtime": [
        "dist/jsx-dev-runtime.d.ts"
      ],
      "transition": [
        "dist/transition/index.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "scripts/vite-plugin-route-types.ts"
  ],
  "keywords": [
    "reactive",
    "ui",
    "framework",
    "typescript",
    "lightweight",
    "spa",
    "router"
  ],
  "author": "snrraptopack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/snrraptopack/auwla.git"
  },
  "homepage": "https://github.com/snrraptopack/auwla#readme",
  "bugs": {
    "url": "https://github.com/snrraptopack/auwla/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:lib && pnpm run build:cli",
    "build:cli": "tsc src/cli.ts --outDir dist --target ES2020 --module ESNext --moduleResolution node --esModuleInterop --allowJs --skipLibCheck",
    "postbuild": "node -e \"require('fs').rmSync('dist/app', { recursive: true, force: true }); require('fs').mkdirSync('dist/transition', { recursive: true }); require('fs').copyFileSync('src/transition/view-transitions.css', 'dist/transition/view-transitions.css')\"",
    "build:lib": "vite build && tsc --project tsconfig.lib.json --emitDeclarationOnly --declaration --outDir dist",
    "build:app": "vite build --config vite.config.app.ts",
    "preview": "vite preview",
    "preview:app": "vite preview --outDir dist-app",
    "template:parse": "bun run template/test/test-parser.ts",
    "template:test": "bun run template/test/test-full.ts",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "dev:bench:auwla": "pnpm -F auwla-bench dev",
    "dev:bench:react": "pnpm -F react-bench dev",
    "dev:bench:vue": "pnpm -F vue-bench dev",
    "preview:bench:auwla": "pnpm -F auwla-bench preview",
    "preview:bench:react": "pnpm -F react-bench preview",
    "preview:bench:vue": "pnpm -F vue-bench preview"
  },
  "bin": {
    "auwla": "./dist/cli.js",
    "auwla-routes": "./dist/cli.js"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^20",
    "@vitest/coverage-v8": "^1.6.1",
    "c8": "^9.1.0",
    "jsdom": "^24.0.0",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^1.5.0"
  }
}
