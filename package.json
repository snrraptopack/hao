{
  "name": "auwla",
  "version": "0.0.9",
  "description": "A lightweight, reactive UI framework with blazing-fast performance",
  "type": "module",
  "main": "./dist/auwla.umd.cjs",
  "module": "./dist/auwla.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/auwla.js",
      "require": "./dist/auwla.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "reactive",
    "ui",
    "framework",
    "typescript",
    "lightweight",
    "spa",
    "router"
  ],
  "author": "snrraptopack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/snrraptopack/auwla.git"
  },
  "homepage": "https://github.com/snrraptopack/auwla#readme",
  "bugs": {
    "url": "https://github.com/snrraptopack/auwla/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "build:lib": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "preview": "vite preview",
    "template:parse": "bun run template/test/test-parser.ts",
    "template:test": "bun run template/test/test-full.ts"
  },
  "bin": {
    "auwla": "./dist/cli.js",
    "auwla-routes": "./dist/cli.js"
  },
  "devDependencies": {
    "@types/acorn": "^6.0.4",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/bun": "latest",
    "@types/handlebars": "^4.1.0",
    "@vitejs/plugin-react": "^5.0.4",
    "acorn": "^8.15.0",
    "handlebars": "^4.7.8",
    "typescript": "latest",
    "vite": "latest"
  },
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@tailwindcss/vite": "^4.1.14",
    "node-html-parser": "^7.0.1",
    "tailwindcss": "^4.1.14"
  }
}
