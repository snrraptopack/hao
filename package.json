{
  "name": "auwla",
  "version": "0.0.3",
  "description": "A lightweight, reactive UI framework with blazing-fast performance",
  "type": "module",
  "main": "./dist/auwla.umd.cjs",
  "module": "./dist/auwla.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/auwla.js",
      "require": "./dist/auwla.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "reactive",
    "ui",
    "framework",
    "typescript",
    "lightweight",
    "spa",
    "router"
  ],
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:lib": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "vite": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "tailwindcss": "^4.1.14"
  }
}
